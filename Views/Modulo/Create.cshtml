<!-- https://stackoverflow.com/questions/15904653/return-view-with-model-and-go-to-particular-anchor-tag -->
@section Scripts{
    @if (!string.IsNullOrEmpty(ancora))
    {
        <script>
        $(function () {
                window.location.hash = '#@ViewBag.Anchor';
        });
        $(document).ready(function () {
            var anchor = $('#@ViewBag.Anchor');
            anchor.collapse();
            anchor.scrollIntoView(true);
        });
        </script>
    }
    <script>
        function SelezTT() {
            $('input[name=selezionaTutto]').change(
                function () {
                    if ($('input[name=selezionaTutto]').prop('checked')) {
                        var modulo = $('input[name=check]').each(function (index, element) {
                            $(element).prop('checked', true);
                            console.log(index);
                        })
                    }
                    else {
                        var modulo = $('input[name=check]').each(function (index, element) {
                            $(element).prop('checked', false);
                        })
                    }
                });
        };
        $(function () {
            var tutticheck = true;
            $('input[name=check]').change(
                function () {
                    var check = $('input[name=check]');
                    for (i = 0; i < check.length; i++) {
                        if (!check[i].checked) {
                            tutticheck = false;
                            $('input[name=selezionaTutto]').prop('checked', false);
                            break;
                        } else tutticheck = true;
                        if (tutticheck)
                            $('input[name=selezionaTutto]').prop('checked', true);
                    }
                }
            ).change();
        });
    </script> 

    <script>
        var pattern = /^[A-Za-zÀ-ÿ\d\-_\s]+$/i

        $('input[myName="checkCaratteri"]')
            .each(function () {
                $(this).data("oldValue", '')
                $(this).bind('input propertychange', function () {
                    var $this = $(this);
                    var newValue = $this.val();
                    var id = $this.attr('id');
                    if (!pattern.test(newValue)) {
                        if (newValue) {
                            $('span[name=' + id + ']').html("Carattere non ammesso. Inserire solo caratteri alfanumerici, trattini, underscore e spazi.");
                            $('span[name=' + id + ']').show();
                            $this.parent().addClass("has-error");
                        }
                        else
                        	$this.data('oldValue', newValue);
                    }
                    if (pattern.test(newValue)){
                        $('span[name=' + id + ']').html("");
                        $('span[name=' + id + ']').hide();
                        $this.parent().removeClass("has-error");
                        $this.data('oldValue', newValue);
                    }
                    return $this.val($this.data('oldValue'));
                });
            });
    </script>
    }

