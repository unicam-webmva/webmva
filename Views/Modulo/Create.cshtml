                                            <h4 asp-for="@(Model.SERVER.Nome)">Nome Modulo</h4>
                                            <input class="form-control" asp-for="@(Model.SERVER.Nome)" myName="checkCaratteri" id="serverNome"/>
                                            <span class="text-danger" name="serverNome" style="display:none"></span>
                                        </div>
                                    </div>
                                    <div class="col-sm-12">
                                        <h4>Opzioni</h4><hr />
                                        <div class="col-sm-12">
                                            <div class="col-sm-7" style="margin:0 auto; float:none">
                                                <div class="form-group row">
                                                    <h4 asp-for="@(Model.SERVER.ServerIP)">Server IP</h4>
                                                    <input class="form-control" asp-for="@(Model.SERVER.ServerIP)" />
                                                </div>
                                            </div>
                                            <div class="col-sm-7" style="margin:0 auto; float:none">
                                                <div class="form-group row">
                                                    <h4 asp-for="@(Model.SERVER.ServerIP)">Https</h4>
                                                    <input asp-for="@(Model.SERVER.Https)" />
                                                </div>
                                            </div>
                                            <div class="col-sm-7" style="margin:0 auto; float:none">
                                                <div class="form-group row">
                                                    <h4 asp-for="@(Model.SERVER.Porta)">Porta</h4>
                                                    <input type="number" class="form-control" asp-for="@(Model.SERVER.Porta)" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-sm-12">
                                            @if (ViewData["TestS"] != null)
                                            {
                                                <div class="col-sm-8">
                                                    <h5 style="color:red">
                                                        <strong>
                                                            @{
                                                                string check = ViewBag.TestS;
                                                                if (check == "True")
                                                                {
                                                                    @Html.Raw("Il server specificato è attivo");
                                                                }
                                                                else
                                                                {
                                                                    @Html.Raw("Il server specificato è spento<br>");
                                                                }
                                                            }
                                                        </strong>
                                                    </h5>
                                                </div>
                                            }
                                            <div class="col-sm-4  text-right ">
                                                <form asp-action="Test">
                                                    <div class="form-group">
                                                        <input type="submit" asp-action="Test" class="btn btn-default" value="Test" asp-route-cosa="server" asp-route-daDove="Create" />
                                                    </div>
                                                </form>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-sm-12">
                                        <hr />
                                        <div class="col-sm-1"></div>
                                        <div class="col-sm-5 text-right">
                                            <div class="form-group row">
                                                <input type="submit" asp-action="Create" asp-route-cosa="server" class="btn btn-default" value="Crea" />&nbsp;&nbsp; |
                                                <a asp-action="Index"> Torna alla lista</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
