                                                <input type="submit" asp-action="Edit" class="btn btn-default"  asp-route-id="@Model.DROOPE.ID" value="Salva" />&nbsp;&nbsp;|
                                                <a asp-action="Index"> Torna alla lista</a>
                                            </div>
                                        </div>
                                        </div>
                    </div>
                </div>
            </div>
        </form>
    }
    @* -----INFOGA----- *@
