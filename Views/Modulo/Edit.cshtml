     @* -----INFOGAEMAIL----- *@
    else if (infogaemail)
    {
        <h2><em>Modifica modulo</em><strong> @(Model.INFOGAEMAIL.Nome)</strong></h2>
        <hr />
        <form asp-action="Edit" name="infogaeForm">
                                <input type="hidden" asp-for="@Model.INFOGAEMAIL.Applicazione" value="@webmva.Models.APPLICAZIONE.INFOGAEMAIL" />
                                <input type="hidden" asp-for="@Model.INFOGAEMAIL.ID" />
 <div class="row panel panel-default panel-body">
                                <div class="col-sm-12">
