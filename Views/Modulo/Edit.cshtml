                        </div>
                    </div>

                    <hr />
                    <div class="col-sm-12">
                        <div class="col-sm-7" style="margin:0 auto; float:none">
                            <div class="form-group row">
                                
                                    <h4 asp-for="@(Model.DROOPE.ComandoPersonalizzato)">Comando personalizzato</h4>
                                
                                <input class="form-control" asp-for="@Model.DROOPE.ComandoPersonalizzato" />
                            </div>
                            <div class="form-group row">

                            </div>
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <h4>Tipi di enumerazione</h4>
                        <hr />
                        <div class="form-group row">
                            <div class="col-sm-4">
                                <label class="control-label">Scegli cms</label>
                            </div>
                            <select class="col-sm-5" asp-for="@Model.DROOPE.Cms">
