        List<string> ListaModuliSenzaTarget = ViewBag.ListaSenzaTarget;
        List<string> ListaModuliSenzaInserito = ViewBag.ListaSenzaInserito;
        string messaggio = ViewBag.ProgettoSenzaModuli;
        if (ListaModuliSenzaTarget != null && ListaModuliSenzaTarget.Count != 0)
        {
            @Html.Raw("<h3 style=\"color:red\"><em>Inserire il target in:</em> <strong>" + ListaModuliSenzaTarget[0]);
            for (int i = 1; i < ListaModuliSenzaTarget.Count; i++)
            {
                @Html.Raw(", " + ListaModuliSenzaTarget[i]);
            }
            @Html.Raw("</strong></h3>");
        }
        if (ListaModuliSenzaInserito != null && ListaModuliSenzaInserito.Count != 0)
        {
            @Html.Raw("<h3 style=\"color:red\"><em>Inserire il check in:</em> <strong>" + ListaModuliSenzaInserito[0]);
            for (int i = 1; i < ListaModuliSenzaInserito.Count; i++)
            {
                @Html.Raw(", " + ListaModuliSenzaInserito[i]);
            }
            @Html.Raw("</strong></h3>");
        }
        if (!string.IsNullOrEmpty(messaggio))
        {
            @Html.Raw("<h3 style=\"color:red\"><em>" + messaggio + "</em></h3>");
        }
    }
